<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath Server.Principal 8082 5 localhost 8080 localhost 8081 localhost 8083 localhost 8084 localhost 8085 localhost 8086 localhost 8087 localhost 8088</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath Server.Principal 8082 5 localhost 8080 localhost 8081 localhost 8083 localhost 8084 localhost 8085 localhost 8086 localhost 8087 localhost 8088</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath Server.Principal 8082 5 localhost 8080 localhost 8081 localhost 8083 localhost 8084 localhost 8085 localhost 8086 localhost 8087 localhost 8088</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
    </actions>
